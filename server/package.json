{
  "name": "vision-ai-server-app",
  "version": "1.0.0",
  "description": "The VisionAI App is a machine learning-powered image recognition system.",
  "main": "dist/index.js",
  "author": "Dave-LaFlam",
  "scripts": {
    "dev": "NODE_ENV=development tsx -r tsconfig-paths/register src/index.ts",
    "clean": "rimraf build",
    "build": "tsc && tsc-alias",
    "postbuild": "node replace.config.js",
    "start": "node -r tsconfig-paths/register build/index.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "logs:pm2": "pm2 logs vision-ai-server-app",
    "prod": "pnpm start:pm2 && pnpm logs:pm2",
    "prod:stop": "pm2 stop vision-ai-server-app",
    "prod:restart": "pm2 restart vision-ai-server-app",
    "lint": "eslint . --ignore-pattern node_modules --ignore-pattern dist",
    "lint:fix": "eslint --ignore-pattern node_modules --ignore-pattern dist --fix .",
    "test": "NODE_ENV=test PORT=3001 pnpm jest --clearCache && pnpm jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "pnpm jest --clearCache && jest --coverage"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@pinecone-database/pinecone": "4.1.0",
    "@tensorflow-models/mobilenet": "2.1.1",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "@xenova/transformers": "2.17.2",
    "body-parser": "1.20.3",
    "canvas": "3.1.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "flatted": "3.3.2",
    "openai": "4.69.0",
    "safe-stable-stringify": "^2.5.0",
    "sharp": "0.33.5",
    "tsconfig-paths": "4.2.0",
    "undici": "^7.3.0",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.75",
    "@types/node-fetch": "2.6.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "replace-in-file": "^8.3.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tensorflow/tfjs-node"
    ]
  }
}
